<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="10dp"
    android:gravity="center"
    android:layout_marginRight="10dp"
    android:orientation="vertical" >
    
	<Button 
	    android:id="@+id/id_room_manage"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:text="烤房管理" />
	
	<Button 
	    android:id="@+id/id_new_tobacco"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:text="鲜烟管理" />
		
	<Button 
	    android:id="@+id/id_dry_tobacco"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:text="干烟管理" />
	
	<Button 
	    android:id="@+id/id_equipment_manage"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:text="烘烤物资管理" />
	
	<Button 
	    android:id="@+id/id_user_manage"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:text="人员管理" />
	
	<Button 
	    android:id="@+id/id_arbitrate"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:text="争议仲裁" />
	
	<Button 
	    android:id="@+id/id_search"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:text="烘烤统计查询" />
	
</LinearLayout>
